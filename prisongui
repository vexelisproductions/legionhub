workspace.Remote.TeamEvent:FireServer("Bright blue")

for i = 10,500 do
    for i,v in pairs(Workspace.Prison_ITEMS.clothes:GetChildren()) do
 
lol = Workspace.Remote.ItemHandler:InvokeServer(v.ITEMPICKUP)
print(lol)
end
end
end
end)

